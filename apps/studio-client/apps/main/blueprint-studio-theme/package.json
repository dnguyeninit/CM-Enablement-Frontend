  "private": true,
  "dependencies": {
    "@coremedia/studio-client.ext.studio-theme": "2110.1.0",
    "@jangaroo/runtime": "^1.0.0"
  },
  "devDependencies": {
    "@jangaroo/build": "^1.0.0",
    "@jangaroo/core": "^1.0.0",
    "@jangaroo/publish": "^1.0.0",
    "rimraf": "^3.0.2"
